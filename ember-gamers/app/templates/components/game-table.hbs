<table class="table table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Publisher</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
        {{#each games as |game|}}
            <tr>
                <td>{{game.title}}</td>
                <td>{{game.publisher}}</td>
                <td>{{game.rating}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
